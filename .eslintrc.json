{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "import/no-extraneous-dependencies": "off",
      "import/no-unresolved": ["error", {"commonjs": false}],
      "import/extensions": ["error", "ignorePackages"],
      "no-underscore-dangle": "off",
      "no-console": "off"
    }
}
